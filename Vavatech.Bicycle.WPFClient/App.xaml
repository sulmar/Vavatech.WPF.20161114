             xmlns:local="clr-namespace:Vavatech.Bicycle.WPFClient"
             StartupUri="Views/StationsView.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources\Styles.xaml" />
